<script src="/js/jquery.min.js"></script>
<script src="/js/foundation.min.js"></script>
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<script>
    $(document).foundation();

    AOS.init({
        duration: 1050,
        once: true,
        delay: 20
    });

    setTimeout(function () {
        AOS.refresh();
    }, 0);
</script>